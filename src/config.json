{
	"orchestra": {
		"version": "1.0.2"
	},
	"docker": {
		"machine": {
			"orchestra": {
				"tld": "snapshots.dpm.sjc.io"
			}
		}
	},
	"soundEffects": {
		"volume": 0.25
	},
	"test": {
		"timeout": 100
	},
	"message": {
		"error": {
			"char": "❗",
			"colour": "red",
			"sound": "bad"
		},
		"success": {
			"char": "✅",
			"colour": "green",
			"sound": "good"
		},
		"default": {
			"char": "⭐",
			"colour": false,
			"sound": "neutral"
		}
	}
}
